<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee 
                             http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    
 <servlet>
	<servlet-name>login</servlet-name>
	<servlet-class>com.customer.LoginServlet</servlet-class>
 </servlet>
 <servlet-mapping>
 	<servlet-name>login</servlet-name>
 	<url-pattern>/log</url-pattern>
 </servlet-mapping>
 
 <servlet>
	<servlet-name>Customerinsert</servlet-name>
	<servlet-class>com.customer.CustomerInsert</servlet-class>
 </servlet>
 <servlet-mapping>
 	<servlet-name>Customerinsert</servlet-name>
 	<url-pattern>/Customerinsert</url-pattern>
 </servlet-mapping>


<servlet>
    <servlet-name>RoomUploadServlet</servlet-name>
    <servlet-class>com.example.servelet.RoomUploadServelet</servlet-class>
</servlet>
<servlet-mapping>
    <servlet-name>RoomUploadServlet</servlet-name>
    <url-pattern>/uploadRoom</url-pattern>
</servlet-mapping>

<servlet>
    <servlet-name>RoomDisplayServlet</servlet-name>
    <servlet-class>com.example.servelet.RoomDisplayServlet</servlet-class>
</servlet>
<servlet-mapping>
    <servlet-name>RoomDisplayServlet</servlet-name>
    <url-pattern>/RoomDisplayServlet</url-pattern>
</servlet-mapping>

 <servlet>
	<servlet-name>updateCustomerServlet</servlet-name>
	<servlet-class>com.customer.UpdateCustomerServlet</servlet-class>
 </servlet>
 <servlet-mapping>
 	<servlet-name>updateCustomerServlet</servlet-name>
 	<url-pattern>/update</url-pattern>
 </servlet-mapping>

<servlet>
	<servlet-name>deleteCustomerServlet</servlet-name>
	<servlet-class>com.customer.deleteCustomerServlet</servlet-class>
 </servlet>
 <servlet-mapping>
 	<servlet-name>deleteCustomerServlet</servlet-name>
 	<url-pattern>/delete</url-pattern>
 </servlet-mapping>


</web-app>
